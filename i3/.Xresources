/* *.font: xft:Iosevka:style=Regular:size=12 */
*.font: xft:Iosevka Term,Iosevka Nerd Font Mono:style=Regular:size=12
*.boldFont: xft:Iosevka:style=Bold:size=12
*.italicFont: xft:Iosevka:style=Italic:size=12
*.boldItalicFont: xft:Iosevka:style=Bold Italic:size=12

URxvt.perl-lib: /home/enermis/.config/urxvt/perl-lib/
URxvt.perl-ext-common: default,tabbed,matcher,resize-font,-tabbed
URxvt.font: xft:SF Mono:size=12,xft:Inconsolata Nerd Font Mono:size=12

! Restore Ctrl+Shift+(c|v)
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.scrollBar: false

